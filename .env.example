# App Configuration
APP_ENV=development
DEBUG=True
LOG_LEVEL=INFO

# Aliyun DashScope (Qwen) API Key
DASHSCOPE_API_KEY=sk-xxxxxx

# Tavily Search API Key (Used for real-time web searching)
# Get one at https://tavily.com/
TAVILY_API_KEY=tvly-xxxxxx

# TianYanCha (TYC) API Token (Used for official enterprise info)
# Get one at https://open.tianyancha.com/
TIANYANCHA_TOKEN=xxxxxx-xxxx-xxxx-xxxx-xxxxxx

# Model Names
MODEL_ROUTER_NAME=qwen-turbo
MODEL_EXPERT_NAME=qwen-max
EMBEDDING_MODEL_NAME=text-embedding-v3

# Database & Storage
SQLITE_DB_PATH=sqlite:///data/greencredit.db
